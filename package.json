{
  "name": "written-number",
  "version": "1.0.1-beta",
  "description": "Convert numbers to words - their written form.",
  "main": "lib/index.js",
  "scripts": {
    "build": "webpack --display-error-details",
    "lint": "eslint ./",
    "lint:fix": "npm run lint -- --fix",
    "test": " mocha --compilers js:@babel/register -R mocha-spec-cov-alt"
  },
  "keywords": [
    "written",
    "number",
    "english",
    "int",
    "word",
    "words",
    "numbers to words",
    "human",
    "spoken"
  ],
  "author": "Pedro Tacla Yamada",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamadapc/js-written-number"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "blanket": "1.1.7",
    "clean-webpack-plugin": "^3.0.0",
    "grunt": "^1.0.3",
    "grunt-browserify": "^5.3.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-uglify": "^2.3.0",
    "mocha": "^3.5.3",
    "mocha-spec-cov-alt": "^1.1.1",
    "mversion": "^1.10.0",
    "should": "^11.2.1",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6"
  },
  "config": {
    "blanket": {
      "data-cover-never": [
        "node_modules"
      ],
      "pattern": [
        "lib"
      ],
      "spec-cov": {
        "threshold": 80,
        "localThreshold": 80,
        "lcovOutput": "lcov"
      }
    }
  },
  "dependencies": {
    "@babel/register": "^7.5.5"
  }
}
